    {
      "request": {
        "method": "POST",
        "url": "/with/body",
        "bodyPatterns": [
            { "matches": "<status>OK</status>" },
            { "doesNotMatch": ".*ERROR.*" }
        ]
    },
    "response": {
            "status": 270,
            "body":"This is a POST Request"
    }
},
  {
    "request": {
        "method": "POST",
        "url": "/with/headers",
        "headers": {
            "Content-Type": {
                "equalTo": "text/xml"
            },
            "Accept": {
                "matches": "text/json"
            },
            "etag": {
                "doesNotMatch": "abcd.*"
            },
            "X-Custom-Header": {
                "contains": "2134"
            }
        }
    },
    "response": {
            "status": 250,
            "body" : "This request URL has headers"
    }
},
  {
    "request": {
        "method": "GET",
        "urlPath": "/with/query",
        "queryParameters": {
            "search": {
                "contains": "Some text"
            },
           "searchtext": {
                "contains": "shelton"
            }
        }
    },
    "response": {
            "status": 230,
            "body": "This is a URL Path"
    }
